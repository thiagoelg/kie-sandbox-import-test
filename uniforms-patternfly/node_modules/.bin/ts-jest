#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thiagolugli/ibm/kie-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/thiagolugli/ibm/kie-tools/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.8.4/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@26.5.6_jest@26.6.3_typescript@4.8.4/node_modules/ts-jest/cli.js" "$@"
fi
